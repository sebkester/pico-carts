pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
function _init()
 mx=0
 my=0
 mh=64
 mw=128
 
	player_init()
	baddie_init()
	cls()
	print (pp)
end

function _update()
 player_update()
 baddie_update()
 
 mx = -64 + flr((pp[1].x + pp[2].x)/2)
 my = -64 + flr((pp[1].y + pp[2].y)/2)
	if (abs(pp[1].y - pp[2].y) > 32*8) then
  my = my - 32 * 8
	end
	if (abs(pp[1].x - pp[2].x) > 64*8) then
  mx = mx - 64 * 8
	end

end

function _draw()
 cls()
 map(flr(mx/8)-1, flr(my/8)-1, (-8-mx%8), (-8-my%8))
	
	if (my < 0) then
  map(flr(mx/8)-1, flr(my/8)+(mh-1), -8-mx%8, -8-my%8)
	end
	if (mx < 0) then
  map(flr(mx/8)+(mw-1), flr(my/8)-1, -8-mx%8, -8-my%8)
	end
	if (mx < 0 and my < 0) then
  map(flr(mx/8)+(mw-1), flr(my/8)+(mh-1), -8-mx%8, -8-my%8)
	end
	
 for p in all(pp) do
  draw_player(p)
 end
	
 for b in all(bb) do
  draw_player(b)
 end
 
 bbb = {false,false}
 print (pp[1].x)
 print (abs(pp[1].y - pp[2].y))
 print (-my)
 print (-my % 8)
 print (flr(my/8))
 print (bb[1].bfn[1])
end

-->8
-- player code
function player_init()

	pp = {}
	add(pp,new_player(80,64,1))
	add(pp,new_player(88,64,2))
end

function new_player(x,y,sprite)
 p = {}
 p.x = x
 p.y = y
 p.v = 1
 p.sprite = sprite
 p.bfn = btn
 return p
end

function player_update()
 update_player(pp[1],0)
 update_player(pp[2],1)
end

function update_player(p,i)
 if (bbtn(p.bfn,0,i) and sclear(p.x-p.v, p.y)) then
  p.x = (p.x - p.v) % (128*8)
 end
 if (bbtn(p.bfn,1,i) and sclear(p.x+p.v, p.y)) then
  p.x = (p.x + p.v) % (128*8)
 end
 if (bbtn(p.bfn,2,i) and sclear(p.x, p.y-p.v)) then
 	p.y = (p.y - p.v) % (64*8)
 end
 if (bbtn(p.bfn,3,i) and sclear(p.x, p.y+p.v)) then
 	p.y = (p.y + p.v) % (64*8)
 end
end

function sclear(x,y)
 return clear(x,y) and
  clear(x+7,y) and
  clear(x,y+7) and
  clear(x+7,y+7)
end

function clear(x,y)
 --if (x<0 or y<0) return false
 -- blocked?
 m = mget((x/8)%mw,(y/8)%mh)
 if (fget(m,0)) return false
 
 return true
end

function draw_player(p)
 spr(p.sprite,p.x-mx, p.y-my)
 spr(p.sprite,p.x-mx-mw*8, p.y-my)
 spr(p.sprite,p.x-mx, p.y-my-mh*8)
 spr(p.sprite,p.x-mx-mw*8, p.y-my-mh*8 + 63)
end
-->8
-- baddies
function baddie_init()

	bb = {}
	bv=0.2
	add_baddie(128,96,34)
	add_baddie(16,96,34)
	add_baddie(216,296,33)
	add_baddie(208,296,33)
end

function add_baddie(x,y,s)
 b = new_player(x,y,s)
 b.v = bv;
 b.bfn = {false,false,false,false,false,false}
 add(bb,b)
end

function baddie_update()

 for b in all(bb) do
  chase(b, closest_player(b))
  update_player(b,2)

  //b.x = b.x + bv
 end
end

function closest_player(b)
	d1 = (pp[1].x-b.x)^2 + (pp[1].y-b.y)^2
	d2 = (pp[2].x-b.x)^2 + (pp[2].y-b.y)^2
	if (d1<d2) return pp[1]
	return pp[2]
end

function chase(b,p)
 b.bfn = {false,false,false,false,false,false}
 if (p.x < b.x-1) b.bfn[0+1]=true
 if (p.x > b.x+1) b.bfn[1+1]=true
 if (p.y < b.y-1) b.bfn[2+1]=true
 if (p.y > b.y+1) b.bfn[3+1]=true
end

function bbtn(fn,i,p)
 if (type(fn) == "function") return fn(i,p)
 return fn[i+1]
end


