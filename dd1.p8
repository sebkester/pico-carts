__lua__
function _init()
 mx=0
 my=0
	player_init()
	cls()
	print (pp)
end

function _update()
 player_update()
 
 mx = flr((-128 + pp[1].x + pp[2].x)/8/2)
 my = flr((-128 + pp[1].y + pp[2].y)/8/2)
end

function _draw()
 cls()
 map(mx,my,0,0)
 for p in all(pp) do
  spr(p.sprite,p.x - mx*8, p.y-my*8)
 end
 
end

-->8
-- player code
function player_init()

	pp = {}
	add(pp,new_player(8,8,1))
	add(pp,new_player(16,8,2))
end

function new_player(x,y,sprite)
 p = {}
 p.x = x
 p.y = y
 p.v = 1
 p.sprite = sprite
 return p
end

function player_update()
 update_player(pp[1],0)
 update_player(pp[2],1)
end
function update_player(p,i)
 if (btn(0,i) and sclear(p.x-p.v, p.y)) then
  p.x = p.x - p.v
 end
 if (btn(1,i) and sclear(p.x+p.v, p.y)) then
  p.x = p.x + p.v
 end
 if (btn(2,i) and sclear(p.x, p.y-p.v)) then
 	p.y = p.y - p.v
 end
 if (btn(3,i) and sclear(p.x, p.y+p.v)) then
 	p.y = p.y + p.v
 end
end

function sclear(x,y)
 return clear(x,y) and
  clear(x+7,y) and
  clear(x,y+7) and
  clear(x+7,y+7)
end

function clear(x,y)
 if (x<0 or y<0) return false

 -- blocked?
 m = mget(x/8,y/8)
 if (fget(m,0)) return false
 
 return true
end
