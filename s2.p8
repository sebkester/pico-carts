--enemy code--
function enemy_init()
 ee = {}
 for i=1,10 do
  new_enemy()
 end
end

function new_enemy()
 e = {}
 e.x = rnd(128)
 e.y = rnd(64)
 e.v = 1
 e.sprite = 48
 e.exploding = false
 e.hyperspacing = true
 
 add(ee, e)

end

function update_enemy()

 for e in all(ee) do
  if e.exploding then
   e.sprite += 0.5
   if e.sprite > 35 then
    del(ee, e)
   end
  elseif e.hyperspacing then
   e.sprite += 0.5
   if e.sprite > 51 then
    e.sprite = 17
    e.hyperspacing = false
   end
  else
  
   if rnd(10) > 9 then
	   if (e.y > p.y) e.y -= e.v
	   if (e.y < p.y) e.y += e.v
   end

			if rnd(10) > 9 then   
	   if (e.x > p.x) e.x -= e.v
	   if (e.x < p.x) e.x += e.v
   end
   
   if (rnd(120) < 1) enemy_fire(e)
  end
 end
 
 if (rnd(10) < 1) new_enemy()
end

function draw_enemy()
 for e in all(ee) do
  spr(e.sprite, e.x, e.y)
 end
end

function explode(e)
 sfx(0)
 e.exploding = true
 e.sprite = 32
end

