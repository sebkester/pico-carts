-- shots
function shots_init()
 ss = {}
end

function fire()
 if (p.firedelay > 0) return
 
 p.firedelay=6
 s = {}
 s.x = p.x
 s.y = p.y-4
 s.v = 4
 s.sprite = 2
 s.ours=true
 
 add(ss, s)
 sfx(1)
end

function enemy_fire(e)
 s = {}
 s.x = e.x
 s.y = e.y+8
 s.v = -1
 s.sprite = 18
 
 add(ss, s)
 sfx(2)
end

function update_shots()
 for s in all(ss) do
  s.y -= s.v
  if (s.y < 0 or s.y > 128) then
   del(ss, s)
  end
 end
end

function draw_shots()
 for s in all(ss) do
  spr(s.sprite, s.x, s.y)
 end
end


