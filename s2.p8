--missile code--
function missile_init()
 ms = {}
end

function launch_missile()
 if (p.launchdelay > 0) return
 
 p.launchdelay=30
 s = {}
 s.x = p.x
 s.y = p.y-4
 s.v = 3
 s.blast_radius = 24
 s.ours=true
 s.exploding = false
 if p.launch_left then
  s.sprite = 4 
 else
  s.sprite = 5
 end
 p.launch_left = not p.launch_left
 
 add(ms, s)
 sfx(4)
end


function update_missiles()
 for s in all(ms) do
  s.y = s.y - s.v
  if (s.y < 0 or s.y > 128 or s.exploding) then
   del(ms, s)
  end
 end
end

function draw_missiles()
 for s in all(ms) do
  if (s.exploding) then
   circfill(s.x, s.y, s.blast_radius, 12)
  else
	  spr(s.sprite, s.x, s.y)
			spr(s.sprite+2, s.x, s.y+8)
		end 		 
 end
end

function make_fragments()

end

