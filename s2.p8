function _init()
 player_init()
 shots_init()
 missile_init()
 enemy_init()
end

function _update()

 update_player()
 update_shots()
 update_missiles()
 update_enemy()
 check_hits()
end

function _draw()
	cls()
	
	if p.alive then
		spr(p.sprite, p.x, p.y)
		if (p.t) then
		 spr(3,p.x, p.y+8)
		end
	end
	
	draw_shots()
	draw_missiles()
	draw_enemy()
	
	if not p.alive then
	 print "game over"
	else
	 for i=1,p.health do
	 
	 	print ("â™¥", i*8-8, 1, 8) 
	 end
	end
	
	print (p.score, 128-24, 1, 5)
end


