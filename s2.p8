-- player code

function player_init()
	p = {}
	p.x = 60
	p.y = 100
	p.v = 2
	p.sprite = 1
 p.firedelay = 0
 p.launchdelay=0
 p.health = 3
 p.exploding = false
 p.alive = true
 p.score = 0
 p.launch_left=false
end

function update_player()
 if p.alive and not p.exploding then
  process_input()
  p.sprite = 1
 elseif p.alive then
  p.sprite += 0.5
  if p.sprite > 35 then
   p.alive = false
  end
 end
end

function process_input()
 p.t = false
 p.firedelay = p.firedelay - 1
 p.launchdelay = p.launchdelay - 1
 if (btn(0)) p.x -= p.v
 if (btn(1)) p.x += p.v
 if (btn(2)) then
  p.t = true
  p.y -= p.v
 end
 if (btn(3)) p.y += p.v
 if (btn(4)) fire()
 if (btn(5)) launch_missile()
 if (p.x < 0) p.x = 0
 if (p.y < 0) p.y = 0
 if (p.x > 128-8) p.x = 128-8
 if (p.y > 128-8) p.y = 128-8
end

function damage()
 p.health -= 1
 sfx(0)
 p.sprite = 32
 if p.health < 0 then
  explode(p)
 end
end


