-- collision code
function is_hit(a, b, r)
 if (not r) r=5
 if b.x > a.x-r
  and b.x < a.x+r 
  and b.y > a.y-r
  and b.y < a.y+r then
   return true
 end

 return false
end

function check_hits()
 for e in all(ee) do
  if not e.exploding 
   and not e.hyperspacing then
			-- hit by a shot?
	  for s in all(ss) do
		  if is_hit(s,e) then
		   del(ss,s)
					blow_up_enemy(s,e)
		  end
		 end  
		 
		 -- hit by a missle?
		 for s in all(ms) do
		  if is_hit(s,e) then
		   -- del(ms,s)
		   s.exploding = true
					blow_up_enemy(s,e)
					for t in all(ee) do
					 if is_hit(s,t,s.blast_radius) then
					  blow_up_enemy(s,t)
					 end
					end
					--make_fragments(e)
		  end
		 end 
	
	  -- hit by another enemy?
	  for b in all(ee) do
		  if b != e and is_hit(b,e) then
		   explode(e)
		   explode(b)
		  end
		 end  
		 
		 -- hit by player?
		 if is_hit(p,e) then
		  explode(e)
		  damage(p)
		 end
  end
 end
 
 -- player hit by shot?
 if p.alive then
	 for s in all(ss) do
	  if is_hit(s,p) then
	   del(ss,s)
	   damage(p)
	  end
	 end 
 end
end

function blow_up_enemy(s,e)
 if (not e.exploding) then
  explode(e)
 end
 if (s.ours) then
  p.score += 1
  if (p.score % 25) == 0 then
   p.health += 1
   sfx(3)
  end
 end
end
